max :: Int -> Int -> Int;
max x y = if x > y then x else y;

min :: Int -> Int -> Int;
min x y = if x < y then x else y;

range :: Int -> Int -> Int;
range n x = if n < x then (range (n + 1) x) else n;

main :: IO;
